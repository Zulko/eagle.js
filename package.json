{
  "name": "eagle.js",
  "version": "0.2.0",
  "description": "Slideshow framework for Vue.js",
  "main": "dist/eagle.cjs.js",
  "module": "dist/eagle.es.js",
  "url": "https://github.com/zulko/eagle.js",
  "repository": "https://github.com/Zulko/eagle.js",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "slideshow",
    "slide",
    "presentation",
    "vue.js"
  ],
  "author": "Zulko",
  "license": "ISC",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "node build/build.js",
    "lint": "eslint --ext vue,js --format node_modules/eslint-friendly-formatter 'src/components/**' src/main.js",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "@/([^\\.]*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  },
  "dependencies": {
    "animate.css": "3.5.2",
    "highlight.js": "9.9.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-plugin-html": "^2.0.3",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "jest": "^21.2.1",
    "jest-vue-preprocessor": "^1.3.1",
    "node-sass": "^4.5.0",
    "pug": "^2.0.0-beta11",
    "rollup": "^0.59.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-vue": "^2.3.1",
    "shelljs": "^0.7.6",
    "vue": "^2.1.10",
    "vue-template-compiler": "^2.1.10"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
