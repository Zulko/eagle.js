    "sass-loader": "^7.1.0",
    "shelljs": "^0.7.6",
    "style-loader": "^0.23.0",
    "vue": "^2.5.17",
    "vue-loader": "^14.2.3",
    "vue-template-compiler": "^2.1.10"
